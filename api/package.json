{
    "name": "api",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "private": true,
    "allowJs": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --runInBand --detectOpenHandles",
        "testci-api": "jest src/api --json --reporters=jest-junit --runInBand --detectOpenHandles --outputFile=results-api.xml",
        "testci-bll": "jest src/bll --json --reporters=jest-junit --runInBand --detectOpenHandles --outputFile=results-bll.xml",
        "testci-dalman": "jest src/dal/manualTests --json --reporters=jest-junit --runInBand --detectOpenHandles --outputFile=results-dalman.xml",
        "testci-dalgen": "jest src/dal/generatedTests --json --reporters=jest-junit --runInBand --detectOpenHandles --outputFile=results-dalgen.xml",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@hubspot/api-client": "^3.4.1",
        "@nestjs-modules/mailer": "^1.5.1",
        "@nestjs/common": "^7.6.14",
        "@nestjs/core": "^7.6.14",
        "@nestjs/jwt": "^7.2.0",
        "@nestjs/mapped-types": "^0.1.1",
        "@nestjs/passport": "^7.1.5",
        "@nestjs/platform-express": "^7.6.14",
        "@nestjs/schedule": "^0.4.3",
        "@nestjs/swagger": "^4.7.16",
        "@types/mssql": "^6.0.7",
        "@types/multer": "^1.4.5",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "fundscraper-model-enums": "file:../fundscraper-model-enums",
        "helmet": "^4.4.1",
        "html-entities": "^2.1.1",
        "mssql": "^6.3.1",
        "multer": "^1.4.2",
        "nestjs": "0.0.1",
        "node-sql-parser": "^3.1.0",
        "nodejs-md5": "^1.1.0",
        "nodemailer": "^6.5.0",
        "passport": "^0.4.1",
        "passport-azure-ad": "^4.3.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.6",
        "swagger-ui-express": "^4.1.6",
        "xlsx": "^0.16.9",
        "zip-a-folder": "^1.0.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.5.6",
        "@nestjs/schematics": "^7.3.0",
        "@nestjs/testing": "^7.6.14",
        "@types/express": "^4.17.11",
        "@types/jest": "26.0.10",
        "@types/microsoft__typescript-etw": "^0.1.1",
        "@types/node": "^13.13.47",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "3.9.1",
        "@typescript-eslint/parser": "3.9.1",
        "eslint": "7.7.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.20.1",
        "jest": "26.4.2",
        "jest-fetch": "^1.1.1",
        "jest-junit": "^12.0.0",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-jest": "26.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.9"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "testTimeout": 120000,
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "reporters": [
            "default",
            "jest-junit"
        ]
    }
}
